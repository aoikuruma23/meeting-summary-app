{
  "routes": [
    { "src": "/favicon.ico", "dest": "/favicon.svg" },
    { "src": "/manifest.json", "headers": { "Cache-Control": "no-cache" } },
    { "src": "/sw.js", "headers": { "Cache-Control": "no-cache", "Service-Worker-Allowed": "/" } },
    { "src": "/icons/.*", "headers": { "Cache-Control": "public, max-age=31536000, immutable" } },
    { "src": "/assets/.*", "headers": { "Cache-Control": "public, max-age=31536000, immutable" } },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
